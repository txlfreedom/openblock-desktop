{
  "name": "openblock-desktop",
  "productName": "Scratch",
  "description": "Openblock as a self-contained desktop application",
  "author": "Openblock Team",
  "version": "2.2.5",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf ./dist ./statics ./external-resources ./tools ./translations ./firmwares ./drivers",
    "i18n:src": "mkdirp translations/core && format-message extract --out-file translations/core/en.json src/main/**.js",
    "i18n:push": "tx-push-src openblock-editor desktop translations/core/en.json",
    "start": "mkdirp ./dist && electron-webpack dev --bail --display-error-details --env.minify=false --no-progress",
    "compile": "mkdirp ./dist && electron-webpack --bail --display-error-details --env.minify=false --no-progress",
    "fetch:drivers": "rimraf ./drivers && node scripts/download-driver.js",
    "fetch:exts": "rimraf ./external-resources && node ./node_modules/openblock-resource/script/download.js",
    "fetch:firmwares": "rimraf ./firmwares && node ./node_modules/openblock-link/script/download-firmwares.js",
    "fetch:tools": "rimraf ./tools && node ./node_modules/openblock-link/script/download-tools.js",
    "fetch:statics": "rimraf ./statics && mkdirp ./statics && git clone https://github.com/openblockcc/openblock-asset.git statics",
    "fetch:all": "npm run fetch:drivers && npm run fetch:exts && npm run fetch:firmwares && npm run fetch:tools && npm run fetch:statics",
    "fetch:all32": "npm run fetch:drivers && npm run fetch:exts && npm run fetch:firmwares && npm run fetch:tools -- --arch=ia32 && npm run fetch:statics",
    "build": "npm run build:dev",
    "build:dev": "npm run compile && npm run doBuild -- --mode=dev",
    "build:dir": "npm run compile && npm run doBuild -- --mode=dir",
    "build:dist": "npm run compile && npm run doBuild -- --mode=dist",
    "build:dist32": "npm run compile && npm run doBuild -- --mode=dist --arch=ia32",
    "doBuild": "node ./scripts/electron-builder-wrapper.js",
    "dist": "npm run clean && npm run compile && npm run fetch:all && npm run doBuild -- --mode=dist",
    "dist32": "npm run clean && npm run compile && npm run fetch:all32 && npm run doBuild -- --mode=dist --arch=ia32",
    "distDev": "npm run clean && npm run compile && npm run fetch:all && npm run doBuild -- --mode=dev",
    "test": "npm run test:lint",
    "test:lint": "eslint --cache --color --ext .jsx,.js src scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/openblockcc/openblock-desktop.git"
  },
  "dependencies": {
    "openblock-link": "0.1.0-prerelease.20210727145234",
    "openblock-resource": "0.1.0-prerelease.20210529040649",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "async": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-intl": "^7.5.7",
    "compare-versions": "^3.6.0",
    "copy-webpack-plugin": "^5.1.1",
    "cpx": "^1.5.0",
    "del": "^6.0.0",
    "download-github-release": "^0.3.2",
    "electron": "^8.5.5",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.0.0",
    "electron-notarize": "^0.3.0",
    "electron-store": "^5.1.1",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.0.0",
    "eslint-config-scratch": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.20.0",
    "format-message": "^6.2.3",
    "format-message-cli": "^6.2.3",
    "fs-extra": "^9.0.1",
    "intl": "1.2.5",
    "lodash.bindall": "^4.4.0",
    "lodash.defaultsdeep": "^4.6.1",
    "minilog": "^3.1.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "nets": "^3.2.0",
    "node-localstorage": "^2.1.6",
    "npm-force-resolutions": "0.0.10",
    "openblock-gui": "github:openblockcc/openblock-gui#openblock-desktop-v2.2.3",
    "openblock-l10n": "^3.10.20210714115626",
    "os-locale": "^5.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "2.9.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "uuid": "^8.3.2",
    "webpack": "^4.43.0"
  },
  "resolutions": {
    "upath": "^1.0.5",
    "usb": "1.6.5"
  }
}
